{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "dom"],
    "types": ["cypress", "node", "cypress-xpath"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@badeball/cypress-cucumber-preprocessor/*": [
        "./node_modules/@badeball/cypress-cucumber-preprocessor/dist/subpath-entrypoints/*"
      ],
      "@support/*": ["cypress/support/*", "cypress/support/custom.d.ts"]
    }
  },
  "include": ["**/*.ts", "cypress/support/custom.d.ts"]
}
